{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from '@/api/axios'; // Путь к вашему настроенному экземпляру Axios\n\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      confirmPassword: '',\n      errorMessage: '',\n      message: '',\n      // Сообщение об успешной регистрации\n      isRegisterMode: false // Флаг для определения режима (авторизация или регистрация)\n    };\n  },\n  computed: {\n    formTitle() {\n      return this.isRegisterMode ? 'Регистрация' : 'Авторизация';\n    },\n    submitButtonText() {\n      return this.isRegisterMode ? 'Зарегистрироваться' : 'Войти';\n    }\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const url = this.isRegisterMode ? '/register' : '/login';\n        const data = {\n          username: this.username,\n          password: this.password\n        };\n        if (this.isRegisterMode) {\n          data.confirmPassword = this.confirmPassword;\n        }\n        console.log('Отправка данных:', data);\n        const response = await axios.post(url, data);\n        console.log('Ответ сервера:', response.data);\n        if (response.status === 200) {\n          if (this.isRegisterMode) {\n            this.message = 'Успешная регистрация'; // Устанавливаем сообщение об успешной регистрации\n            setTimeout(() => {\n              this.message = ''; // Через некоторое время очищаем сообщение\n            }, 2000); // Примерно через 2 секунды\n          }\n          localStorage.setItem('token', response.data.token); // Сохранение токена в localStorage\n          this.$router.push('/'); // Переход на главную страницу\n        }\n      } catch (error) {\n        if (error.response && error.response.status === 400) {\n          this.errorMessage = error.response.data.message || 'Неправильное имя пользователя или пароль';\n        } else {\n          this.errorMessage = 'Ошибка при отправке формы';\n        }\n        console.error('Ошибка отправки формы:', error);\n      }\n    },\n    setLoginMode() {\n      this.isRegisterMode = false;\n      this.errorMessage = '';\n    },\n    setRegisterMode() {\n      this.isRegisterMode = true;\n      this.errorMessage = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","confirmPassword","errorMessage","message","isRegisterMode","computed","formTitle","submitButtonText","methods","submitForm","url","console","log","response","post","status","setTimeout","localStorage","setItem","token","$router","push","error","setLoginMode","setRegisterMode"],"sources":["/home/aleksandr/PraktikaSportmasterLabQuiz/frontend/src/components/CommonForm.vue"],"sourcesContent":["<template>\n  <div class=\"auth-container\">\n    <h2>{{ formTitle }}</h2>\n\n    <!-- Кнопки выбора операции -->\n    <div class=\"operation-buttons\">\n      <button @click=\"setLoginMode\">Авторизация</button>\n      <button @click=\"setRegisterMode\">Регистрация</button>\n    </div>\n\n    <form v-if=\"!isRegisterMode\" @submit.prevent=\"submitForm\" class=\"auth-form\">\n      <div class=\"form-field\">\n        <label>Имя пользователя:</label>\n        <input type=\"text\" v-model=\"username\">\n      </div>\n      <div class=\"form-field\">\n        <label>Пароль:</label>\n        <input type=\"password\" v-model=\"password\">\n      </div>\n      <button type=\"submit\">{{ submitButtonText }}</button>\n    </form>\n\n    <form v-if=\"isRegisterMode\" @submit.prevent=\"submitForm\" class=\"auth-form\">\n      <div class=\"form-field\">\n        <label>Имя пользователя:</label>\n        <input type=\"text\" v-model=\"username\">\n      </div>\n      <div class=\"form-field\">\n        <label>Пароль:</label>\n        <input type=\"password\" v-model=\"password\">\n      </div>\n      <div class=\"form-field\">\n        <label>Подтвердите пароль:</label>\n        <input type=\"password\" v-model=\"confirmPassword\">\n      </div>\n      <button type=\"submit\">{{ submitButtonText }}</button>\n\n      <!-- Вывод сообщения об успешной регистрации -->\n      <p v-if=\"message\">{{ message }}</p>\n    </form>\n\n    <p v-if=\"errorMessage\">{{ errorMessage }}</p>\n  </div>\n</template>\n\n<script>\nimport axios from '@/api/axios'; // Путь к вашему настроенному экземпляру Axios\n\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      confirmPassword: '',\n      errorMessage: '',\n      message: '', // Сообщение об успешной регистрации\n      isRegisterMode: false, // Флаг для определения режима (авторизация или регистрация)\n    };\n  },\n  computed: {\n    formTitle() {\n      return this.isRegisterMode ? 'Регистрация' : 'Авторизация';\n    },\n    submitButtonText() {\n      return this.isRegisterMode ? 'Зарегистрироваться' : 'Войти';\n    }\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const url = this.isRegisterMode ? '/register' : '/login';\n        const data = {\n          username: this.username,\n          password: this.password\n        };\n\n        if (this.isRegisterMode) {\n          data.confirmPassword = this.confirmPassword;\n        }\n\n        console.log('Отправка данных:', data);\n\n        const response = await axios.post(url, data);\n        console.log('Ответ сервера:', response.data);\n\n        if (response.status === 200) {\n          if (this.isRegisterMode) {\n            this.message = 'Успешная регистрация'; // Устанавливаем сообщение об успешной регистрации\n            setTimeout(() => {\n              this.message = ''; // Через некоторое время очищаем сообщение\n            }, 2000); // Примерно через 2 секунды\n          }\n          localStorage.setItem('token', response.data.token); // Сохранение токена в localStorage\n          this.$router.push('/'); // Переход на главную страницу\n        }\n      } catch (error) {\n        if (error.response && error.response.status === 400) {\n          this.errorMessage = error.response.data.message || 'Неправильное имя пользователя или пароль';\n        } else {\n          this.errorMessage = 'Ошибка при отправке формы';\n        }\n        console.error('Ошибка отправки формы:', error);\n      }\n    },\n    setLoginMode() {\n      this.isRegisterMode = false;\n      this.errorMessage = '';\n    },\n    setRegisterMode() {\n      this.isRegisterMode = true;\n      this.errorMessage = '';\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Стили для компонента */\n.auth-container {\n  max-width: 400px;\n  margin: auto;\n  padding: 20px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  background-color: #f2f9ff; /* Голубой фон */\n}\n\nh2 {\n  text-align: center;\n}\n\n.auth-form {\n  display: flex;\n  flex-direction: column;\n}\n\n.form-field {\n  display: flex;\n  flex-direction: column;\n  margin-bottom: 10px;\n}\n\nlabel {\n  margin-bottom: 5px;\n}\n\ninput[type=\"text\"],\ninput[type=\"password\"] {\n  width: 100%;\n  padding: 8px;\n  margin-bottom: 10px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  box-sizing: border-box; /* Добавлено для обеспечения правильного расчета ширины */\n}\n\nbutton {\n  width: 100%;\n  background-color: #007bff; /* Голубой цвет кнопки */\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  cursor: pointer;\n  border-radius: 4px;\n  margin-top: 10px;\n}\n\nbutton:hover {\n  background-color: #0056b3; /* Темно-голубой цвет при наведении */\n}\n\n.operation-buttons {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 20px; /* Увеличенный отступ между кнопками и формой */\n}\n\n.operation-buttons button {\n  margin: 0 10px; /* Добавленный отступ между кнопками */\n}\n\np {\n  color: green;\n  margin-top: 10px;\n}\n</style>\n"],"mappings":";AA8CA,OAAOA,KAAI,MAAO,aAAa,EAAE;;AAEjC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,YAAY,EAAE,EAAE;MAChBC,OAAO,EAAE,EAAE;MAAE;MACbC,cAAc,EAAE,KAAK,CAAE;IACzB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAA