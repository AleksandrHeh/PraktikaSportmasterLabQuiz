>\n      <div v-if=\"showResult\" class=\"quiz-result\">\n        <h2>Результаты викторины</h2>\n        <p>Вы набрали {{ score }} баллов из {{ questions.length }}.</p>\n        <p v-if=\"discountMessage\">{{ discountMessage }}</p>\n        <button @click=\"restartQuiz\" class=\"restart-button\">Начать заново</button>\n      </div>\n\n      <div v-else-if=\"currentQuestion\" class=\"quiz-question\">\n        <h2>{{ currentQuestion.question_text }}</h2>\n        <ul>\n          <li v-for=\"answer in currentQuestion.answers\" :key=\"answer.answer_id\">\n            <button @click=\"checkAnswer(answer)\" class=\"answer-button\">{{ answer.answer_text }}</button>\n          </li>\n        </ul>\n      </div>\n\n      <div v-else class=\"loading\">\n        Loading...\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      questions: [],\n      currentQuestionIndex: 0,\n      score: 0,\n      showResult: false,\n      currentUser: null, // Добавленное свойство для хранения текущего пользователя\n      discountMessage: '' // Сообщение о скидке\n    };\n  },\n  computed: {\n    currentQuestion() {\n      return this.questions[this.currentQuestionIndex];\n    }\n  },\n  methods: {\n    async fetchQuestions() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/questions');\n        this.questions = response.data;\n      } catch (error) {\n        console.error('Error fetching questions:', error);\n      }\n    },\n    async fetchCurrentUser() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/user'); // Замените на свой API endpoint для получения данных о пользователе\n        this.currentUser = response.data;\n      } catch (error) {\n        console.error('Error fetching current user:', error);\n      }\n    },\n    checkAnswer(answer) {\n      if (answer.is_correct) {\n        this.score++;\n      }\n      if (this.currentQuestionIndex + 1 < this.questions.length) {\n        this.currentQuestionIndex++;\n      } else {\n        this.showResult = true;\n        this.calculateDiscount();\n      }\n    },\n    calculateDiscount() {\n      const discount = this.score * 10; // 10% скидка за каждый балл\n      if (discount > 0) {\n        this.discountMessage = `Поздравляем! Вы получаете скидку ${discount}%.`;\n      } else {\n        this.discountMessage = `К сожалению, вы не набрали достаточное количество баллов для получения скидки.`;\n      }\n    },\n    restartQuiz() {\n      this.currentQuestionIndex = 0;\n      this.score = 0;\n      this.showResult = false;\n      this.discountMessage = '';\n    }\n  },\n  mounted() {\n    this.fetchQuestions();\n    this.fetchCurrentUser(); // Получение данных о текущем пользователе при загрузке компонента\n  }\n};\n</script>\n\n<style scoped>\n/* Стили для вашего сайта */\n.quiz-container {\n  background-color: #f7f7f7;\n  border-radius: 10px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nheader {\n  background-color: #1E2ED4;\n  color: #FFF;\n  padding: 20px;\n  text-align: center;\n  border-radius: 10px 10px 0 0;\n}\n\n.header-content {\n  max-width: 100%;\n  margin: 0 auto;\n  padding: 0 20px; /* Добавлено для центрирования контента в шапке */\n}\n\nh1 {\n  font-size: 1.8em;\n  margin: 0;\n}\n\n.content {\n  max-width: 600px;\n  margin: 20px auto; /* Центрирование основного контента */\n  padding: 20px;\n}\n\n.quiz-question,\n.quiz-result,\n.loading {\n  margin-top: 20px;\n  padding: 20px;\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.quiz-question h2,\n.quiz-result h2 {\n  margin-bottom: 20px;\n  font-size: 1.5em;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  margin-bottom: 10px;\n}\n\n.answer-button,\n.restart-button {\n  background-color: #3F51B5;\n  color: #FFF;\n  padding: 10px 15px;\n  margin: 5px 0;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n  font-size: 1em;\n  width: 100%;\n  text-align: left;\n}\n\n.answer-button:hover,\n.restart-button:hover {\n  background-color: #303F9F;\n}\n\n.answer-button:focus,\n.restart-button:focus {\n  outline: none;\n  box-shadow: 0 0 0 3px rgba(63, 81, 181, 0.5);\n}\n\n.loading {\n  text-align: center;\n  padding: 20px;\n}\n\n.user-info {\n  margin-right: 20px;\n  color: #fff;\n  font-weight: bold;\n}\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAgB;;EAElBA,KAAK,EAAC;AAAgB;gEACzBC,mBAAA,CAAoB,YAAhB,aAAW;;EAJvBC,GAAA;EAKgCF,KAAK,EAAC;;;EAM7BA,KAAK,EAAC;AAAS;;EAXxBE,GAAA;EAY6BF,KAAK,EAAC;;gEAC3BC,mBAAA,CAA6B,YAAzB,sBAAoB;;EAbhCC,GAAA;AAAA;;EAAAA,GAAA;EAmBuCF,KAAK,EAAC;;oBAnB7C;;EAAAE,GAAA;EA4BkBF,KAAK,EAAC;;;uBA3BtBG,mBAAA,CA+BM,OA/BNC,UA+BM,GA9BJH,mBAAA,CAOS,iBANPA,mBAAA,CAKM,OALNI,UAKM,GAJJC,UAAoB,EACTC,KAAA,CAAAC,WAAW,I,cAAtBL,mBAAA,CAEM,OAFNM,UAEM,EAAAC,gBAAA,CADDH,KAAA,CAAAC,WAAW,CAACG,QAAQ,oBANjCC,mBAAA,e,KAWIX,mBAAA,CAoBM,OApBNY,UAoBM,GAnBON,KAAA,CAAAO,UAAU,I,cAArBX,mBAAA,CAKM,OALNY,UAKM,GAJJC,UAA6B,EAC7Bf,mBAAA,CAA+D,WAA5D,aAAW,GAAAS,gBAAA,CAAGH,KAAA,CAAAU,KAAK,IAAG,aAAW,GAAAP,gBAAA,CAAGH,KAAA,CAAAW,SAAS,CAACC,MAAM,IAAG,GAAC,iBAClDZ,KAAA,CAAAa,eAAe,I,cAAxBjB,mBAAA,CAAmD,KAf3DkB,UAAA,EAAAX,gBAAA,CAeqCH,KAAA,CAAAa,eAAe,oBAfpDR,mBAAA,gBAgBQX,mBAAA,CAA0E;IAAjEqB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;IAAExB,KAAK,EAAC;KAAiB,eAAa,E,KAGnDyB,QAAA,CAAAE,eAAe,I,cAA/BxB,mBAAA,CAOM,OAPNyB,UAOM,GANJ3B,mBAAA,CAA4C,YAAAS,gBAAA,CAArCe,QAAA,CAAAE,eAAe,CAACE,aAAa,kBACpC5B,mBAAA,CAIK,c,kBAHHE,mBAAA,CAEK2B,SAAA,QAxBfC,WAAA,CAsB+BN,QAAA,CAAAE,eAAe,CAACK,OAAO,EAAjCC,MAAM;yBAAjB9B,mBAAA,CAEK;MAF0CD,GAAG,EAAE+B,MAAM,CAACC;QACzDjC,mBAAA,CAA4F;MAAnFqB,OAAK,EAAAa,MAAA,IAAEV,QAAA,CAAAW,WAAW,CAACH,MAAM;MAAGjC,KAAK,EAAC;wBAAmBiC,MAAM,CAACI,WAAW,wBAvB5FC,WAAA,E;uDA4BMnC,mBAAA,CAEM,OAFNoC,WAEM,EAFsB,cAE5B,G","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}