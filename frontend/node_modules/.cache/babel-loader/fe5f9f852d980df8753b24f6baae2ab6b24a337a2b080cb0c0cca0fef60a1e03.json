страницу входа\n      this.$router.push('/login');\n    }\n\n    this.fetchQuestions();\n  }\n};\n</script>\n\n<style scoped>\n/* Стили для компонента */\n</style>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,oBAAoB,EAAE,CAAC;MACvBC,KAAK,EAAE,CAAC;MACRC,UAAU,EAAE,KAAK;MACjBC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACP,SAAS,CAAC,IAAI,CAACC,oBAAoB,CAAC;IAClD;EACF,CAAC;EACDO,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,qCAAqC,CAAC;QACvE,IAAI,CAACX,SAAQ,GAAIU,QAAQ,CAACX,IAAI;MAChC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MACxD;IACF,CAAC;IACDE,WAAWA,CAACC,MAAM,EAAE;MAClB,IAAIA,MAAM,CAACC,UAAU,EAAE;QACrB,IAAI,CAACd,KAAK,EAAE;MACd;MACA,IAAI,IAAI,CAACD,oBAAmB,GAAI,IAAI,IAAI,CAACD,SAAS,CAACiB,MAAM,EAAE;QACzD,IAAI,CAAChB,oBAAoB,EAAE;MAC7B,OAAO;QACL,IAAI,CAACE,UAAS,GAAI,IAAI;MACxB;IACF,CAAC;IACDe,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACjB,oBAAmB,GAAI,CAAC;MAC7B,IAAI,CAACC,KAAI,GAAI,CAAC;MACd,IAAI,CAACC,UAAS,GAAI,KAAK;IACzB,CAAC;IACDgB,MAAMA,CAAA,EAAG;MACPC,YAAY,CAACC,UAAU,CAAC,OAAO,CAAC;MAChCD,YAAY,CAACC,UAAU,CAAC,UAAU,CAAC;MACnC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,KAAI,GAAIL,YAAY,CAACM,OAAO,CAAC,OAAO,CAAC;IAC3C,IAAID,KAAK,EAAE;MACT;MACA,IAAI,CAACrB,QAAO,GAAIgB,YAAY,CAACM,OAAO,CAAC,UAAU,CAAC;MAChD,IAAI,CAACrB,QAAO,GAAI,IAAI;IACtB,OAAO;MACL;MACA,IAAI,CAACiB,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;IAEA,IAAI,CAACd,cAAc,CAAC,CAAC;EACvB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}