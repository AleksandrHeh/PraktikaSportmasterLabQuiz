{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      questions: [],\n      currentQuestionIndex: 0,\n      score: 0,\n      showResult: false,\n      username: '',\n      loggedIn: false\n    };\n  },\n  computed: {\n    currentQuestion() {\n      return this.questions[this.currentQuestionIndex];\n    }\n  },\n  methods: {\n    async fetchQuestions() {\n      try {\n        const response = await axios.get('http://localhost:3000/api/questions');\n        this.questions = response.data;\n      } catch (error) {\n        console.error('Ошибка при получении вопросов:', error);\n      }\n    },\n    checkAnswer(answer) {\n      if (answer.is_correct) {\n        this.score++;\n      }\n      if (this.currentQuestionIndex + 1 < this.questions.length) {\n        this.currentQuestionIndex++;\n      } else {\n        this.showResult = true;\n      }\n    },\n    restartQuiz() {\n      this.currentQuestionIndex = 0;\n      this.score = 0;\n      this.showResult = false;\n    },\n    logout() {\n      localStorage.removeItem('token');\n      localStorage.removeItem('username');\n      this.$router.push('/login');\n    }\n  },\n  mounted() {\n    // Проверяем наличие токена в localStorage\n    const token = localStorage.getItem('token');\n    if (token) {\n      // Получаем логин пользователя из localStorage\n      this.username = localStorage.getItem('username');\n      this.loggedIn = true;\n    } else {\n      // Если нет токена, редиректим на страницу входа\n      this.$router.push('/login');\n    }\n    this.fetchQuestions();\n  }\n};","map":{"version":3,"names":["axios","data","questions","currentQuestionIndex","score","showResult","username","loggedIn","computed","currentQuestion","methods","fetchQuestions","response","get","error","console","checkAnswer","answer","is_correct","length","restartQuiz","logout","localStorage","removeItem","$router","push","mounted","token","getItem"],